<UserControl
    x:Class="Music8.NowPlayingCanvas"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Music8"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400" Background="Black"
    Name="RootControl">

    <Grid Name="ShapeContainer">
        <Grid Name="ImageContainer" Background="Black">
            <Rectangle Name="BackgroundColor" Fill="#ff000000" Stretch="Fill" Opacity=".2"/>
            
        </Grid>

        <Rectangle Name="lawl2" Fill="Blue" Width="40" Height="40" Opacity=".2" Margin="80, 80, 0, 0"/>
        <Rectangle Name="lawl" Fill="Blue" Width="10" Height="10" Opacity=".2" Margin="0" HorizontalAlignment="Left"/>
    </Grid>

</UserControl>
